<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.della.djoin"
    android:versionCode="1"
    android:versionName="1.0" >

    <uses-sdk
        android:minSdkVersion="21"
        android:targetSdkVersion="23" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.example.della.djoin.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat" >
        <activity
            android:name="com.example.della.djoin.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.example.della.djoin.Register"
            android:label="Register" >
        </activity>
        <activity
            android:name="com.example.della.djoin.AddTrip"
            android:label="Add Trip" >
        </activity>
        <activity
            android:name="com.example.della.djoin.MyTrips"
            android:label="My Trips" >
        </activity>
        <activity
            android:name="com.example.della.djoin.MyInformation"
            android:label="My Information" >
        </activity>
        <activity
            android:name="com.example.della.djoin.EditInformation"
            android:label="Edit Information" >
        </activity>
        <activity
            android:name="com.example.della.djoin.SearchTrips"
            android:label="Search Trips" >
        </activity>
    </application>

</manifest>