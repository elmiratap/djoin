<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.della.djoin" >

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />

    <application
        android:name="com.example.della.djoin.Application"
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/Theme.AppCompat" >
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".Register"
            android:label="Register" >
        </activity>
        <activity
            android:name=".AddTrip"
            android:label="Add Trip" >
        </activity>
        <activity
            android:name=".MyTrips"
            android:label="My Trips" >
        </activity>
        <activity
            android:name=".MyInformation"
            android:label="My Information" >
        </activity>
        <activity
            android:name=".EditInformation"
            android:label="Edit Information">
        </activity>
        <activity
            android:name=".SearchTrips"
            android:label="Search Trips" >
        </activity>
    </application>

</manifest>
